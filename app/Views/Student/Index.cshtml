@using System.Web.Optimization
@model IEnumerable<repository.registration>
@{
   
    Layout = "~/Views/Shared/StdLayout.cshtml";
}
<div align="center">
      Upcoming Events
       <br>
    @if (Model.Count() == 0)
    {
        @:None. Wait till teacher assigns one to you
    }
     else {   
        @:<table>
            <tr  align="center"><td>Event Name</td><td>Start Time</td><td>End Time</td></tr>
        foreach (var item in Model) {
        
            
                
           <tr  align="center"> 
              <td> <a id="links2" href="~/Student/Start?evnt_id=@item.evnt.evnt_id">@item.evnt.evnt_name</a> </td> 
               <td> 
                   @item.evnt.evnt_sdt </td> 
                  <td> 
                      @item.evnt.evnt_edt </td> 
                  </tr>
           
        }

        @:</table>
        @TempData["error"]
    }
</div>