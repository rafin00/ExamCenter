@model IEnumerable<repository.evnt>
@{
    ViewBag.Title = "ViewResults";
    Layout = "~/Views/Shared/ThrLayout.cshtml";
}



<div>
<h2>View Results</h2>
    <table>
        <tr>
            <td>Event Id</td><td>Event Name</td>
         </tr>
@foreach (var item in Model)
{
    <tr align="center">
        <td>@item.evnt_id</td>
        <td>@item.evnt_name</td>
           
        <td>
            <a id="links2" href="~/Teacher/ViewEvntResult?evnt_id=@item.evnt_id">View Result</a>
        </td>
       </tr>     
}

 
    
</div>